{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Measurement",
  "description": "The Measurement type",
  "required": ["id"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the Measurement type."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The display name of the Measurement type."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the Measurement type."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the Measurement type was last updated."
    }
  }
}
